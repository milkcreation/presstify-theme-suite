{
  "name": "presstify-plugins/theme-suite",
  "description": "Theme Suite PresstiFy plugin - Components collection.",
  "homepage": "https://svn.tigreblanc.fr/presstify-plugins/theme-suite/",
  "keywords": [
    "presstify",
    "plugins",
    "theme suite",
    "partial drivers",
    "field drivers",
    "metabox drivers"
  ],
  "type": "presstify-plugin",
  "time": "2020-11-25",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.tigreblanc.fr/presstify/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "presstify/framework": ">=1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "tiFy\\Plugins\\ThemeSuite\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tiFy\\Plugins\\ThemeSuite\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
